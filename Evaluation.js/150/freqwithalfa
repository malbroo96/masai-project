function countingProblem(s) {
    let makeobj = {};

    // Count each character
    for (let i = 0; i < s.length; i++) {
        let ch = s[i];
        if (makeobj[ch]) {
            makeobj[ch] += 1;
        } else {
            makeobj[ch] = 1;
        }
    }

    // Get keys in lexicographical order
    let sortedKeys = Object.keys(makeobj).sort();

    // Print each character and its count
    for (let ch of sortedKeys) {
        console.log(ch + " " + makeobj[ch]);
    }
}